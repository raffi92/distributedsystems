package server;

import java.net.*;
import java.io.*;

import protocol.Protocol;

public class Server {
	static int port;
	
	public static void main(String[] args){
		port = Protocol.getServerPort();
		try {
			ServerSocket server = new ServerSocket(port);
			Protocol.reply(server);
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
