package client;

import java.util.Scanner;

public class test {
	public static void main (String[] args){
        double PI = 3.14159;
        double R;
        Scanner scan = new Scanner(System.in);
        R = scan.nextInt();
        double A = PI * R * R;
        System.out.println("A=" + String.format( "%.4f", A ));
}
}
